{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "no-console": "off",
    "max-len": [
      "error",
      {
        "code": 120,
        "tabWidth": 2
      }
    ],
    "no-var": "error",
    "arrow-parens": ["error", "as-needed"]
  }
}
